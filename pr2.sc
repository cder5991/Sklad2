def binaryAddition(s1: String, s2: String): String = {
  if (s1 == null || s2 == null) return ""
  var first = s1.length - 1
  var second = s2.length - 1
  val sb = new StringBuilder
  var carry = 0
  while ( {
    first >= 0 || second >= 0
  }) {
    var sum = carry
    if (first >= 0) {
      sum += s1.charAt(first) - '0'
      first -= 1
    }
    if (second >= 0) {
      sum += s2.charAt(second) - '0'
      second -= 1
    }
    carry = sum >> 1
    sum = sum & 1
    sb.append(if (sum == 0) '0'
    else '1')
  }
  if (carry > 0) sb.append('1')
  sb.reverse
  String.valueOf(sb)
}
